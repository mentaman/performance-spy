    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.5",
    "rollup": "^2.62.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "tslint": "^6.1.3",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "re-reselect": "^4.0.0",
