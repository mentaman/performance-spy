{
  "name": "performance-spy",
  "version": "1.0.0",
  "main": "./lib/index.js",
  "jsnext:main": "./es/index.js",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "files": [
    "lib",
    "src",
    "es"
  ],
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./lib/index.js"
    },
    "./resolver/redux-thunk-module": {
      "import": "./es/resolver/redux-thunk-module.js",
      "require": "./lib/resolver/redux-thunk-module.js"
    },
    "./resolver/reselect-module": {
      "import": "./es/resolver/reselect-module.js",
      "require": "./lib/resolver/reselect-module.js"
    },
    "./resolver/re-reselect-module": {
      "import": "./es/resolver/re-reselect-module.js",
      "require": "./lib/resolver/re-reselect-module.js"
    }
  },
  "description": "Performance spy - spy on common libraries and check their performance",
  "scripts": {
    "build": "rm -fr lib && rm -rf es && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^17.0.2",
    "@types/redux": "^3.6.0",
    "jest": "^27.4.5",
    "re-reselect": "^4.0.0",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.5",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "re-reselect": "^4.0.0",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.5"
  }
}
