{
  "name": "performance-spy",
  "version": "0.0.19",
  "main": "./dist/lib/index.js",
  "jsnext:main": "./dist/es/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "files": [
    "readme.md",
    "resolver/",
    "dist/"
  ],
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/lib/index.js"
    }
  },
  "description": "Performance spy - spy on common libraries and check their performance",
  "homepage": "https://github.com/mentaman/performance-spy",
  "repository": {
    "type": "git",
    "url": "https://github.com/mentaman/performance-spy.git"
  }
  "keywords": [
    "performance",
    "profiler",
    "spy"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && npm run resolver-post-build",
    "resolver-post-build": "node resolver-fixer.js",
    "lint": "tslint -c tslint.json 'src/**/*.ts'"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^17.0.2",
    "@types/redux": "^3.6.0",
    "jest": "^27.4.5",
    "re-reselect": "^4.0.0",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.5",
    "tslint": "^6.1.3",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "re-reselect": "^4.0.0",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.5"
  }
}
